<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="External API" parallel="tests" thread-count="5" preserve-order="true">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
</listeners>

<test name="CHMM Refund" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.CHMM_Refund"/>
    </classes>
</test>

<test name="CHMM AE" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.CHMM_AE"/>
    </classes>
</test>

<test name="Expedia" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.Expedia"/>
    </classes>
</test>

<test name="GST" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.Gst"/>
    </classes>
</test>

<test name="Intl Provisonal Book" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.Expedia_Intl_Search"/>
    </classes>
</test>

<test name="Cancellation Policy" preserve-order="true" >
    <classes>
      <class name="com.cleartrip.hotel.extapi.Cancellation_Free"/>
      <class name="com.cleartrip.hotel.extapi.Cancellation_NonRefundable"/>
      <class name="com.cleartrip.hotel.extapi.Cancellation_Policy"/>
    </classes>
</test>

</suite>