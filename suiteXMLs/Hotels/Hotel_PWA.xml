<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MobileWeb PWA" parallel="tests" thread-count="3">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
</listeners>

 	<test name="Cancel Acct Step2 SignIN " preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.Acct_Cancel_SignINStep2"/> 
    </classes>
  	</test> 
  	
  	<test name="GST and HotelName Search" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.GST_HotelName_Search"/> 
    </classes>
  	</test> 
  	  	   
  	<test name="Coupon HomePage SignIN Policy" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.Coupon_SignIN_HomePage_Policy"/> 
    </classes>
  	</test> 
  
  	<test name="Intl NB" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.Intl_NB_Only"/> 
    </classes>
  	</test> 

  <test name="Dom NB" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.Dom_NBRetry"/> 
    </classes>
  </test> 
  
  <test name="MultiDays PAX SRP Hotel Search Unsigned" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.MultiDays_Pax_SRP_HotelName"/> 
    </classes>
  </test>   
  
   <test name="ModifyDate RoomRate Page" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.ModifyDate_RoomRatePage"/> 
    </classes>
  </test>
        
  	<test name="StoredCard" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.StoredCard"/> 
    </classes>
  	</test>  	
  	
  	<test name="Expedia" preserve-order="true">
    <classes>
      <class name="com.cleartrip.hotel.pwa.Expedia_Booking"/> 
    </classes>
  	</test> 
    
</suite> 