<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Visa cases" thread-count="5" parallel="classes"
	verbose="3" preserve-order="true">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults" />
	</listeners>
	
	<test name="Visa">
	<classes>
			    <class name="com.cleartrip.local.visa.IndiaVisaSignedIn" />
			    <class name="com.cleartrip.local.visa.IndiaVisa" />
			    
			    
				<class name="com.cleartrip.local.visa.IndiaVisaCoupon" />
				<class name="com.cleartrip.local.visa.IndiaVisaCouponSignedIn" />
				
			   
				<class name="com.cleartrip.local.visa.PhilipinesVisa" />
			   <class name="com.cleartrip.local.visa.PhilipinesVisaSignedIn" />	
			    
                <class name="com.cleartrip.local.visa.PhilipinesVisaCoupon" />
			    <class name="com.cleartrip.local.visa.PhilipinesVisaCouponSignedIn" />
	</classes>
	
	
	</test>
	
	    
     
  </suite>