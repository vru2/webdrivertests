<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Platfrom Finance" parallel="tests" thread-count="20">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults" />
		<listener class-name="commonServices.RetryListener" />
	</listeners>

	<test name="MIS_Dom_Conformed" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Dom_Cancelled_Trips" />
		</classes>
	</test>


	<test name="MIS_Dom_Departure" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Dom_Departure_Trips" />
		</classes>
	</test>

	<test name="MIS_Intl_Cancelled" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Intl_Cancelled_Trips" />
		</classes>
	</test>

	<test name="MIS_Intl_Conformed" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Intl_Conformed_Trips" />
		</classes>
	</test>

	<test name="MIS_Intl_Departure" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Intl_Departure_Trips" />
		</classes>
	</test>


	<test name="MIS_Dom_Cancelled" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQMISReports_Dom_Cancelled_Trips" />
		</classes>
	</test>

<test name="HQ_Loss_AddEntry" preserve-order="true">
		<classes>
			<class name="testScriptsFinance.HQ_Losstracker_Add_Entry" />
		</classes>
	</test>
	
	
</suite>