<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Accounts Service JAVA API " parallel="tests" preserve-order="true"
	thread-count="5">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults" />
		<listener class-name="commonServices.RetryListener" />
	</listeners>

	<test name="Account_Service_FlyinV2_Signin" parallel="true">
		<classes>
			<class
				name="com.cleartrip.platform.accounts.Account_Service_FlyinV2_Signin" />
		</classes>
	</test>
	
	<test name="Account_Service_FlyinV2_SignUp" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_FlyinV2_SignUP" />
		</classes>
	</test>
	
	<test name="Account_Service_FlyinV2_Social_SignUp" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_FlyinV2_SocialSignup" />
		</classes>
	</test>
	
	
	<test name="Account_Service_FlyinV2_UserSearch" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_FlyinV2_UserSearch" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_ForgotPassword" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_Flyin_ForgotPassword" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_ResetPassword" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_Flyin_ResetPassword" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_Signin" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_Flyin_Signin" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_SignUp" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_Flyin_Signup" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_SocialSignUp" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_Flyin_Social_Signup" />
		</classes>
	</test>
	
	<test name="Account_Service_Flyin_UserSearch" parallel="true">
		<classes>
			<class name="com.cleartrip.platform.accounts.Account_Service_UserSearch" />
		</classes>
	</test>
	
	
</suite>