<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Payment Prod" parallel="tests" thread-count="10">


<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults"/>
		<listener class-name="commonServices.RetryListener"/>
</listeners>


<test name="Payment 3rd Party Wallets" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_AmazonPay"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_FreeCharge"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_JioMoney"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_Mobikwik"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_OlaMoney"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_PayTM"/>
      <class name="testScriptsIndiaHotels.HotelCom_Payment_Wallet_PayU"/>      
    </classes>
</test>

		

<test name="NB Hotels" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsPayments.HotelCom_Prod_Intl_NB"/>
    </classes>
</test> 

<test name="NB Air" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsPayments.AirDom_OW_GDS_NB_Retry_CCBooking"/> 
    </classes>
</test>

		
  </suite> 