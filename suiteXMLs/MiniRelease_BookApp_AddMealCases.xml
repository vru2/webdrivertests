	    	<class name="testScriptsIndia.AirCom_OW_Full_GiftVoucher_Booking"/>  
		</classes> 
	</test>   
		

<test name="ME Air Coupon Cases" parallel="classes">
		<classes>
			<class name="testScriptsMiddleEastAir.MEAir_RT_Coupon_Booking"/>
			<class name="testScriptsMiddleEastAir.MEAir_OW_Coupon_Booking"/>
			<class name="testScriptsMiddleEastAir.MEAir_Intl_OW_Coupon_Booking"/> 
