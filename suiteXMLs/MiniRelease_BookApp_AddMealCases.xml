</classes> 
	</test>   

	
<test name="B2B Coupon Cases"  parallel="tests">
    <classes>        
      	<class name="testScriptsCorpIndia.AirCorp_Dom_OW_Coupon"/>
      	<class name="testScriptsCorpAE.AirCorp_Dom_OW_Coupon"/>
      	<class name="testScriptsAgency.AgencyAir_Coupon"/> 
     </classes>
</test>  
 
	   
<test name="Amex WL Coupon Case" preserve-order="true">
		<classes>
<class name="testScriptsAmexWhiteLabel.AirDom_RT_AmexWhiteLabel_CouponBooking"/>		 	
</classes> 

</test>   
  
  
  
  
  
  
 
  
  
 
</suite>
