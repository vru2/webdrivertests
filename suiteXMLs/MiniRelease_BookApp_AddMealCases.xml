<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mini Release BookApp Coupon Cases" parallel="tests" thread-count="50">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		 <listener class-name="commonServices.RemoveDuplicateResults" />
		<listener class-name="commonServices.RetryListener" />  
	</listeners>

	


 <test name="B2C Air Coupon Cases" preserve-order="true" parallel="classes">
		<classes>
			<class name="testScriptsIndia.AirDom_OW_Coupon_Booking" />
