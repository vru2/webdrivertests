<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="PROD B2B Hotels" parallel="tests" thread-count="50" preserve-order="true" >

<listeners>
		
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
</listeners>

	<test name="Agency" preserve-order="true" parallel="true">
		<classes>
 			<class name="testScriptsAgency.AgencyHotel_NetBanking_Production"/>
 			<class name="testScriptsAgency.AgencyHotel_Intl_Production"/> 			
		</classes>
	</test>

	<test name="Corp.com" preserve-order="true" parallel="true">
		<classes>
	       <class name="testScriptsCorporate.HotelCorp_NetBanking_Production"/>
	       <class name="testScriptsCorporate.HotelCorp_Intl_Production"/>
		</classes>
	</test>
	
	<test name="Corp.ae" preserve-order="true" parallel="true">
		<classes> 
		     <class name="testScriptsCorporate.HotelCorpAE_Production"/>
		     <class name="testScriptsCorporate.HotelCorpAE_Intl_Production"/>
		</classes>
	</test>

</suite> 