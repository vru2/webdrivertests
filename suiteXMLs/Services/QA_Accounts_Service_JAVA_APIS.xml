<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Accounts Service JAVA API - BB " parallel="tests"
	preserve-order="true" thread-count="5">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="common.RemoveDuplicateResults" />
		<listener class-name="common.RetryListener" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>


	<test name="Accounts-Service-PWA" parallel="true">
		<classes>
			<class name="accountsAPI.Accounts_Service_PWA_Fetch_DA_Details" />
			<class name="accountsAPI.Accounts_Service_PWA_register" />
			<class name="accountsAPI.Account_Service_PWA_SignIn_API" />
			<class name="accountsAPI.Account_Service_PWA_GetUser_Byemail" />
			<class name="accountsAPI.Account_Service_PWA_GetUserbyID" />

		</classes>
	</test>


	<test name="Accounts-Service-Userclassification" parallel="true">
		<classes>
			<class name="accountsAPI.Userclassification_Parsing_CSV" />
			<class name="accountsAPI.Userclassification_Userdetails_emailid" />
			<class name="accountsAPI.Userclassification_Gettripdetails_fromtripid" />
			<class name="accountsAPI.Userclassification_Health_Test_Url" />

		</classes>
	</test>

	<test name="Identity-service" parallel="true">
		<classes>
			<class name="accountsAPI.Identtiyservice_get_tokenthroughrefreshToken" />
			<class name="accountsAPI.Identtiyservice_get_tokenthroughCode" />
			<class name="accountsAPI.IdentityService_Getresource" />
			<class name="accountsAPI.IdentityService_AddClient" />

		</classes>
	</test>



	<test name="Account-service-Controllers" parallel="true">
		<classes>
			<class name="accountsAPI.Account_Service_FlyinV2_Signin" />
			<class name="accountsAPI.Account_Service_FlyinV2_SignUP" />
			<class name="accountsAPI.Account_Service_FlyinV2_SocialSignup" />
			<class name="accountsAPI.Account_Service_FlyinV2_UserSearch" />
			<class name="accountsAPI.Account_Service_Flyin_ForgotPassword" />
			<class name="accountsAPI.Account_Service_Flyin_ResetPassword" />
			<class name="accountsAPI.Account_Service_Flyin_Signin" />
			<class name="accountsAPI.Account_Service_Flyin_Signup" />
			<class name="accountsAPI.Account_Service_Flyin_Social_Signup" />
			<class name="accountsAPI.Account_Service_UserSearch" />
			<class
				name="accountsAPI.Account_Service_Partnercontroller_UpdateTraveller" />
			<class name="accountsAPI.Account_Service_Partnercontroller_Usersearch" />
			<class name="accountsAPI.Account_Service_Peoplecontroller_GetuserByEmail" />
			<class name="accountsAPI.Account_Service_Peoplecontroller_GetuserById" />
			<class
				name="accountsAPI.Account_Service_Travellercontroller_UpdateTraveller" />
			<class name="accountsAPI.Account_Service_Peoplecontroller_GetuserById_V2" />

		</classes>
	</test>

	<test name="Account-service-Caching-userprofile" parallel="true">
		<classes>
			<class name="accountsAPI.Account_Service_Caching_Userprofilejson_Email" />
			<class name="accountsAPI.Account_Service_Caching_Userprofilejson_ID" />
			<class name="accountsAPI.Account_Service_Update_User" />
		</classes>

	</test>

