<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Agency and CTPhone" parallel="tests" thread-count="50" preserve-order="true" >

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" /> 
</listeners> 


 <test name="Air : Cancellation" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsAgency.AgencyAir_Dom_OneWay_Cancellation_HQ"/>
    </classes>
</test> 

 
<test name="Air : Netbank Retry" preserve-order="true">
    <classes>
      <class name="testScriptsAgency.AgencyAir_Dom_OneWay_Netbanking_Retry"/>
    </classes>
</test>



<test name="Air : Payment" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsAgency.AgencyAir_Payment_CTPay"/>
    </classes>
</test>


<test name="Air : PAX" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsAgency.AgencyAir_PAX_200"/>
      <class name="testScriptsAgency.AgencyAir_PAX_210"/>
    </classes>
</test>


<test name="SRP and Intinerary Scenarios" preserve-order="true" parallel="true">
    <classes>  
      <class name="testScriptsAgency.Agency_SRP_SendQuotation"/>
      <class name="testScriptsAgency.Agency_Itinerary_FareRule"/>
    </classes>
</test>


<test name="Air : Hold Scenarios" preserve-order="true"  parallel="true" >
    <classes>
      <class name="testScriptsAgency.AgencyAir_Int_Hold_Confirm"/>
      <class name="testScriptsAgency.AgencyAir_Int_Hold_Release"/>
    </classes>
</test>

</suite> 