<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Hotel Booking" parallel="tests" thread-count="1">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="common.RemoveDuplicateResults"/>
		<listener class-name="common.RetryListener"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name="CC Amex" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn_Hotels.IN_AMEX"/>
    </classes>
</test>

<test name="NB ICICI" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn_Hotels.IN_NB_ICICI"/>
    </classes>
</test>


<test name="CC PayU" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn_Hotels.IN_PayU"/>
    </classes>
</test>

<test name="UPI" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn_Hotels.IN_UPI"/>
    </classes>
</test>

</suite>