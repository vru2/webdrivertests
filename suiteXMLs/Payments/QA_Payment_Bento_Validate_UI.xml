<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UI Validation" parallel="tests" thread-count="5">

<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
<listener class-name="common.RemoveDuplicateResults"/>
<listener class-name="common.RetryListener"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name="IN UI" preserve-order="true" >
    <classes>
      <class name="paymentsBento_Itn.Bento_Validation_IN_UI"/> 	  
    </classes>
</test>

<test name="IN Text" preserve-order="true" >
    <classes>
      <class name="paymentsBento_Itn.Bento_Validation_IN_Text"/> 	  
    </classes>
</test>

<test name="AE UI" preserve-order="true" >
    <classes>
      <class name="paymentsBento_Itn.Bento_Validation_AE_UI"/> 	  
    </classes>
</test> 

<test name="AE Text" preserve-order="true" >
    <classes>
      <class name="paymentsBento_Itn.Bento_Validation_AE_Text"/> 	  
    </classes>
</test>

<test name="IN Images and links" preserve-order="true" >
    <classes>
      <class name="paymentsBento_Itn.Bento_Validation_IN_Images_Links"/> 	  
    </classes>
</test>

<test name="Image Validation AE" preserve-order="true" >
    <classes><!-- 
      <class name="paymentsBento_Itn.Validation_Images_AE"/> 	        
      <class name="paymentsBento_Itn.Validation_Text_AE_Arabic"/> -->
    </classes>
</test>

<test name="Wallet" preserve-order="true" >
    <classes><!-- 
      <class name="paymentsBento_Itn.Wallet_Full"/>
      <class name="paymentsBento_Itn.Wallet_Partial"/> -->
    </classes>
</test>

<test name="Coupon" preserve-order="true" >
    <classes><!-- 
      <class name="paymentsBento_Itn.Coupon_Invalid"/> -->
    </classes>
</test>

<test name="ADCB" preserve-order="true" >
    <classes><!-- 
      <class name="paymentsBento_Itn.ADCB_AE"/>    -->
   </classes>
</test>


</suite> 