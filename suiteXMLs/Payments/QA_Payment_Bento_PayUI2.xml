<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Bento UI" parallel="tests" thread-count="2">

<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
<listener class-name="common.RemoveDuplicateResults"/>
<listener class-name="common.RetryListener"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name="UI Validation" preserve-order="true" >
    <classes>
      <class name="paymentsBento_com.Validation_UI_IN"/> 	  
    </classes>
</test>

<test name="UI Validation RT" preserve-order="true" >
    <classes>
      <class name="paymentsBento_com.Validation_UI_IN_RT"/> 	  
    </classes>
</test>

</suite> 