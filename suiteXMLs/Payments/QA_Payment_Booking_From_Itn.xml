<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Bento UI" parallel="tests" thread-count="3">

<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
<listener class-name="common.RemoveDuplicateResults"/>
<listener class-name="common.RetryListener"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name="Booking From Itinerary" preserve-order="true" >
    <classes>
      <class name="paymentsBento_com.Bento_Coupon_Booking"/> 
       <class name="paymentsBento_com.Bento_Full_GV_Booking"/> 
        <class name="paymentsBento_com.Bento_NB_ICICI_Booking"/> 
         <class name="paymentsBento_com.Bento_StoredCard_Booking"/> 
          <class name="paymentsBento_com.Bento_UPI_Booking"/> 
           <class name="paymentsBento_com.Bento_Wallet_Booking"/> 	  
    </classes>
</test>

</suite> 