</test>
<test name="Booking From Itinerary Com Domain" preserve-order="true" >
    <classes>
     <class name="paymentsBento_Itn.Bento_Coupon_StoredCard_Booking"/> 
       <class name="paymentsBento_Itn.Bento_Full_GV_Booking"/> 
        <class name="paymentsBento_Itn.Bento_NB_ICICI_Booking"/> 
          <class name="paymentsBento_Itn.Bento_UPI_Booking"/> 
