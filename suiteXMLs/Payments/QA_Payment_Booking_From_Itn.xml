            <class name="paymentsBento_Itn.Bento_BH_PayU_Booking"/>
            <class name="paymentsBento_Itn.Bento_ME_PayU_Booking"/>
            <class name="paymentsBento_Itn.Bento_OM_PayU_Booking"/>
            <class name="paymentsBento_Itn.Bento_QA_PayU_Booking"/>
    </classes>
</test>

<test name="SA Domain" preserve-order="true" >
    <classes>
            <class name="paymentsBento_Itn.Bento_SA_SC_Payfort_Booking"/>
            <class name="paymentsBento_Itn.Bento_SA_SC_Checkout_Booking"/>
            <class name="paymentsBento_Itn.Bento_SA_PayU_Booking"/>
    </classes>
</test>

<test name="KW Domain" preserve-order="true" >
    <classes>
            <class name="paymentsBento_Itn.Bento_KW_PayU_Booking"/>
            <class name="paymentsBento_Itn.Bento_KW_KNet_Booking"/>
    </classes>
</test>

</suite> 