<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Bento India Domain Bookings" parallel="tests" thread-count="1">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="common.RemoveDuplicateResults"/>
		<listener class-name="common.RetryListener"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

<test name="Debit Card" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_DC_Booking"/> 
    </classes>
</test>

<test name="Razorpay CC" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_Storedcard_Razorpaycard_Booking"/>
    </classes>
</test>

<test name="Amex CC" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_Amex_Booking"/>
    </classes>
</test>

<test name="EMI" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_EMI_Razorpay"/>
    </classes>
</test>

<test name="Coupon PayU" preserve-order="true">
    <classes>
	     	<class name="paymentsBento_Itn.Bento_IN_Coupon_SC_PayU_Booking"/>
    </classes>
</test>

<test name="NB Invalid Coupon" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_InvalidCoupon_NB_ICICI_Booking"/> 
    </classes>
</test>

<test name="Wallet" preserve-order="true">
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_Wallet_Booking"/> 
	        <class name="paymentsBento_Itn.Bento_IN_PartialWallet_AMEX_Booking"/> 
    </classes>
</test>

<test name="Gift Voucher" preserve-order="true">
    <classes>
	       	<class name="paymentsBento_Itn.Bento_IN_Full_GV_Booking"/>
	       	<class name="paymentsBento_Itn.Bento_IN_Partial_GV_Booking"/>
    </classes>
</test>

<test name="UPI" preserve-order="true" >
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_UPI_Booking"/> 
	        <class name="paymentsBento_Itn.Bento_IN_UPI_Scan_Validation"/> 
    </classes>
</test>

<test name="PhonePe Validataion" preserve-order="true" >
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_Phonepe_Validation"/> 
    </classes>
</test>	        

<test name="AmazonPay" preserve-order="true" >
    <classes>
	        <class name="paymentsBento_Itn.Bento_IN_AmazonPay"/> 
    </classes>
</test>

</suite>