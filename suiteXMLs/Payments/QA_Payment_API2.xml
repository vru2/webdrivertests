<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="APIs" parallel="tests" thread-count="2">

<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
<listener class-name="common.RemoveDuplicateResults"/>
<listener class-name="common.RetryListener"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>


<test name="Instant Refund" preserve-order="true" parallel="true">
    <classes>    
      <class name="paymentsAPI.API_IR_Get_Eligibility"/> 
      <class name="paymentsAPI.API_IR_Validate_VPA"/>   
      <class name="paymentsAPI.API_IR_Save_VPA_Details"/> 
      <class name="paymentsAPI.API_IR_Get_Cancelation_Details"/>  
      <class name="paymentsAPI.API_IR_Create_Refund"/>
    </classes>
</test>



</suite> 