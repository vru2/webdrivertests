<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="APIs" parallel="tests" thread-count="2">

<listeners>
<listener class-name="org.uncommons.reportng.HTMLReporter" />
<listener class-name="common.RemoveDuplicateResults"/>
<listener class-name="common.RetryListener"/>
<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>


<test name="Supercoins" preserve-order="true" parallel="true">
    <classes>
      <class name="paymentsAPI.API_SuperCoins_CheckBalance"/>
      <class name="paymentsAPI.API_SuperCoins_CheckEarnPoints"/>   
      <class name="paymentsAPI.API_SuperCoins_Info"/>   
      <class name="paymentsAPI.API_SuperCoins_MobileLinked"/>   
      <class name="paymentsAPI.API_SuperCoins_OTP_Validation_Unhold"/>   
      <class name="paymentsAPI.API_SuperCoins_PromoActivate"/>   
      <class name="paymentsAPI.API_SuperCoins_PromoCreate"/>      
      <class name="paymentsAPI.API_SuperCoins_PromoUpdate"/>      
    </classes>
</test>



</suite> 