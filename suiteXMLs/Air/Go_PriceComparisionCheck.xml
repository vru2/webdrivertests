<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Prod Beta SRP Price Comparision" parallel="tests" thread-count="10">
 
 <listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults"/>
		<listener class-name="commonServices.RetryListener"/>
</listeners>

 <test name="B2C_Air - Domestic Connector Search" preserve-order="true" parallel="true">
 
    <classes>
      <class name="testScriptsPriceComparision.Vistara_Connector_Search"/>
  	  <class name="testScriptsPriceComparision.TruJet_Connector_Search"/>
      <class name="testScriptsPriceComparision.SpiceJet_Connector_Search"/>
      <class name="testScriptsPriceComparision.Radixx_Connector_Search"/>
      <class name="testScriptsPriceComparision.IndiGo_ConnectorCall_Search"/>
      <class name="testScriptsPriceComparision.Connector_9W_Search"/>
      <class name="testScriptsPriceComparision.Amadeus_Connector_Search"/>
      <class name="testScriptsPriceComparision.AirAsia_Connector_Search"/> 
     </classes>
     
</test>

<test name="B2C_Air - International Connector Search" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsPriceComparision.International_OW_Connector_Search"/>
  	 </classes>
</test>

<test name="B2C_Air - Domestic OW FlightCountCheck" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsPriceComparision.API_PriceCompare_B2C_DomOW"/>
      <class name="testScriptsPriceComparision.API_FlightCountCompare_B2C_DomRT"/>
      <class name="testScriptsPriceComparision.API_PriceComapre_B2C_IntlOW"/>
      
  	 </classes>
</test>


<test name="B2C_Air - International FlighCountCompare Search" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsPriceComparision.Internation_OW_FlightCount_Compare"/>
  	 </classes>
</test>


<test name="Hotel - PriceCheck" preserve-order="true" parallel="true">
    <classes>
      <class name="testScriptsIndiaHotels.HotelCom_Price_Comparison_Dom_Bangalore"/>
      <class name="testScriptsIndiaHotels.HotelCom_Price_Comparison_Dom_NewDelhi"/>
      <class name="testScriptsIndiaHotels.HotelCom_Price_Comparison_Dom_Ooty"/>
      <class name="testScriptsIndiaHotels.HotelCom_Price_Comparison_Intl"/>
    </classes>
</test>   












   
