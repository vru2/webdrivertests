<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AIR_SMS_DOM_SG_OW_Suite" parallel="none"
	thread-count="1">
	

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults" />
		<listener class-name="commonServices.RetryListener" />
	</listeners>

	<!-- //Get SSR Required - meal-seat-baggage-insurance-coupon -->

	<parameter name="airline" value="SG" />
	<parameter name="origin" value="BLR" />
	<parameter name="destination" value="AMD" />
	<parameter name="onwardDate" value="15" />
	<parameter name="paymentMethod" value="Credit Card" />
	<parameter name="suppliers" value="SPICEJET" />
	<parameter name="IsFliteTypeInternational" value="false" />
	<parameter name="tripType" value="OW" />

	<test name="Air_DOM_SMS : OW 1 ADT 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="1-0-0" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test name="Air_DOM_SMS : OW 2 ADT 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="2-0-0" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="1-1-0" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 2 ADT 2 CHD 2 INF 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="2-2-2" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF 1 Airline With Meal,SEAT,LUGGAGE,INSURANCE,COUPON Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="1-1-1-1-1" />
		<classes>
			<class
				name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF 1 Airline With Seat,INSURANCE,COUPON and Expressway Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="0-1-0-1-1" />
		<classes>
			<class
				name="testScriptsMicroServices.AirDom_OW_Expressway_Booking_with_SSR">
			</class>
		</classes>
	</test>

	<test name="Air_DOM_SMS : OW 1 ADT within 2 days booking">
		<parameter name="paxConfig" value="1-0-0" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<parameter name="onwardDate" value="1" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF 2 Airline connected Without SSR Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="0-0-0-1-0" />
		<parameter name="tripType" value="connected" />
		<classes>
			<class
				name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF 2 Airline connected With LUGGAGE,INSURANCE,COUPON Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="1-1-1-1-1" />
		<parameter name="tripType" value="connected" />
		<classes>
			<class
				name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF VIA Without SSR Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="0-0-0-1-0" />
		<parameter name="tripType" value="via" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<!-- <test
		name="Air_DOM_SMS : OW 1 ADT 1 CHD 1 INF VIA With MEAL,LUGGAGE,INSURANCE,COUPON Booking">
		<parameter name="paxConfig" value="1-1-1" />
		<parameter name="ssrConfig" value="1-1-1-1-1" />
		<parameter name="tripType" value="via" />
		<classes>
			<class name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test> -->

</suite>
