<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="AIR_SMS_INTL_SG_OW_Suite" parallel="none"
	thread-count="1">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="commonServices.RemoveDuplicateResults" />
		<listener class-name="commonServices.RetryListener" />
	</listeners>

	<!-- //Get SSR Required - meal-seat-baggage-insurance-coupon -->

	<parameter name="airline" value="SG" />
	<parameter name="origin" value="DEL" />
	<parameter name="destination" value="DXB" />
	<parameter name="onwardDate" value="15" />
	<parameter name="paymentMethod" value="Credit Card" />
	<parameter name="suppliers" value="SPICEJET" />
	<parameter name="IsFliteTypeInternational" value="true" />
	<parameter name="tripType" value="OW" />

	<test name="Air_INTL_SMS : OW 1 ADT 1 Airline Without SSR Booking">
		<parameter name="paxConfig" value="1-0-0" />
		<parameter name="ssrConfig" value="0-0-0-0-0" />
		<classes>
			<class
				name="testScriptsMicroServices.Air_Suppliers_Pod_SMS_And_SFF_Flow">
			</class>
		</classes>
	</test>

	<test
		name="Air_INTL_SMS : OW 1 ADT 1 CHD 1 Airline Without SSR Booking">
