<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Local Camp123" thread-count="6" time-out="240000">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>


	<parameter name="user" value="SAProduction"></parameter>
	<parameter name="env" value="prod"></parameter>
	<parameter name="browser" value="chrome"></parameter>

	<test name="Local Camp Others">

		<parameter name="domain" value="com"></parameter>

		<classes>
			<class name="com.cleartrip.local.camp.others.ActivityUnitSA"></class>
			<class name="com.cleartrip.local.camp.others.AddCuisineSA"></class>


		</classes>

	</test>


	<test name="Local Camp Others AE">

		<parameter name="domain" value="ae"></parameter>


		<classes>
			<class name="com.cleartrip.local.camp.others.ActivityUnitSA"></class>
			<class name="com.cleartrip.local.camp.others.AddCuisineSA"></class>


		</classes>

	</test>







</suite> 