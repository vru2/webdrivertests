<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Camp FNB AE" thread-count="6">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>
	<parameter name="domain" value="ae"></parameter>
	<test name="Activities">
		<classes>
			<class
				name="com.cleartrip.local.camp.fnb.CreateActivityPublishUnpublishSA"></class>
			<class name="com.cleartrip.local.camp.fnb.CreateActivityRejectSA"></class>
			<class name="com.cleartrip.local.camp.fnb.PricePerPersonKidsOnlySA"></class>
			<class name="com.cleartrip.local.camp.fnb.PricePerPersonSA"></class>
			<class name="com.cleartrip.local.camp.fnb.PricePerUnitSA"></class>
		</classes>
	</test>
	<test name="ManageData">
		<classes>
			<class name="com.cleartrip.local.camp.fnb.AddBlackOutDatesSA"></class>
			<class name="com.cleartrip.local.camp.fnb.AddChainSA"></class>
			<class name="com.cleartrip.local.camp.fnb.AddFNBTagSA"></class>
			<class name="com.cleartrip.local.camp.fnb.AddVariantSA"></class>
			<class name="com.cleartrip.local.camp.fnb.EditActivitySA"></class>
			<class name="com.cleartrip.local.camp.fnb.EditInventorySA"></class>
			<class name="com.cleartrip.local.camp.fnb.EditVariantPricingSA"></class>
			<class name="com.cleartrip.local.camp.fnb.FNBCustomCollectionSA"></class>
			<class name="com.cleartrip.local.camp.fnb.ManageCategoryAssignSA"></class>
			<class name="com.cleartrip.local.camp.fnb.ManageCategoryOrderSA"></class>
			<class name="com.cleartrip.local.camp.fnb.ReorderSA"></class>
			<class name="com.cleartrip.local.camp.fnb.SortListingsSA"></class>
		</classes>
	</test>
</suite>